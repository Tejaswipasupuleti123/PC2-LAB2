<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="323.0" prefWidth="290.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csd214.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <TextField fx:id="uid" prefHeight="26.0" prefWidth="180.0" text="id" />
    <TextField fx:id="uname" prefHeight="26.0" prefWidth="180.0" text="name" />
    <TextField fx:id="udoctor" prefHeight="26.0" prefWidth="180.0" text="doctor" />
    <TextField fx:id="uroom" prefHeight="26.0" prefWidth="180.0" text="room" />
   <Button mnemonicParsing="false" onAction="#InsertData" text="Insert" />
   <Button mnemonicParsing="false" onAction="#UpdateData" text="Update" />
   <Button mnemonicParsing="false" onAction="#DeleteData" text="Delete" />
    <Button mnemonicParsing="false" onAction="#LoadData" text="LoadData" />
    <Button onAction="#onHelloButtonClick" text="Fetch" />

   <TableView fx:id="tbl_appointmentTable" editable="true" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="id" prefWidth="82.40002822875977" text="id" />
       <TableColumn fx:id="name" prefWidth="81.60003662109375" text="name" />
         <TableColumn fx:id="doctor" prefWidth="56.79998779296875" text="doctor" />
         <TableColumn fx:id="room" prefWidth="27.199972534179693" text="room" />
     </columns>
   </TableView>
</VBox>





